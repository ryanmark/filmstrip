<script src="//cdnjs.cloudflare.com/ajax/libs/pym/0.4.1/pym.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.2.min.js"></script>
<style>
	.filmstrip-outer-wrapper {
	    width: 100%;
	    margin-bottom: 1em;
	 
	    .filmstrip-wrapper {
	        width: 720px;
	        height: 528px;
	    }
	 
	    .filmstrip {
	        width: 100%;
	        height: 100%;
	        -webkit-animation-name: dog;
			-webkit-animation-duration: 2s;
			-webkit-animation-iteration-count: infinite;
			-webkit-animation-timing-function: linear;
			-webkit-animation-timing-function:steps(1, end);
	        background-size: 800px;
	    }
	}
</style>

<div class="filmstrip-outer-wrapper">
	<div class="filmstrip-wrapper">
		<div class="filmstrip animated"></div>
	</div>
</div>

<% for(var i = 0; i < imageUrls.length; i++) { %>
  <img src="<%= imageUrls[i] %>">
<% } %>